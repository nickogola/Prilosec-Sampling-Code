.button {
	-moz-box-shadow:inset 0px 1px 0px 0px #6f994b;
	-webkit-box-shadow:inset 0px 1px 0px 0px #6f994b;
	box-shadow:inset 0px 1px 0px 0px #6f994b;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #548241), color-stop(1, #78a665) );
	background:-moz-linear-gradient( center top, #548241 5%, #78a665 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#548241', endColorstr='#78a665');
	background-color:#548241;
	-webkit-border-top-left-radius:5px;
	-moz-border-radius-topleft:5px;
	border-top-left-radius:5px;
	-webkit-border-top-right-radius:5px;
	-moz-border-radius-topright:5px;
	border-top-right-radius:5px;
	-webkit-border-bottom-right-radius:5px;
	-moz-border-radius-bottomright:5px;
	border-bottom-right-radius:5px;
	-webkit-border-bottom-left-radius:5px;
	-moz-border-radius-bottomleft:5px;
	border-bottom-left-radius:5px;
	text-indent:0px;
	border:1px solid #6b995e;
	display:inline-block;
	color:#ffffff;
	font-family:Arial;
	font-size:15px;
	font-weight:bold;
	font-style:normal;
	height:35px;
	line-height:35px;
	width:120px;
	text-decoration:none;
	text-align:center;
	text-shadow:1px 1px 0px #426110;
}
.button:hover {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #78a665), color-stop(1, #548241) );
	background:-moz-linear-gradient( center top, #78a665 5%, #548241 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#78a665', endColorstr='#548241');
	background-color:#78a665;
}.button:active {
	position:relative;
	top:1px;
}

